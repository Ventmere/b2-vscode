{
  "name": "ventmere-b2",
  "displayName": "Ventmere B2",
  "description": "Integrates Ventmere B2 into VS Code.",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ventmere-b2.init",
    "workspaceContains:/b2config.json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ventmere-b2.init",
        "title": "B2: Initialize Workspace"
      },
      {
        "command": "ventmere-b2.pull",
        "title": "B2: Pull Source Code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "parcel": "^1.12.4",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "b2-sdk": "^1.6.0",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rxjs": "^6.5.3"
  }
}
